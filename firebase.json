{
  "hosting": [
    {
      "target": "student-sys",
      "source": ".",
      "frameworksBackend": {}
    }
  ],
  
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}